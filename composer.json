{
  "name": "oss-tools/laravel-recurring",
  "description": "Package for adding recurring functionality to laravel models.",
  "keywords": [
    "oss-tools",
    "laravel-recurring"
  ],
  "homepage": "https://github.com/oss-tools/laravel-recurring",
  "license": "MIT",
  "require": {
    "php": "^8.0.2",
    "illuminate/database": "^9.0 || ^10.0",
    "illuminate/support": "^9.0 || ^10.0",
    "nesbot/carbon": "^2.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0 || ^8.0",
    "phpunit/phpunit": "^9.5.10 || ^10.1"
  },
  "autoload": {
    "psr-4": {
      "OSSTools\\Recurring\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OSSTools\\Recurring\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "OSSTools\\Recurring\\Providers\\RecurringServiceProvider"
      ]
    }
  }
}
